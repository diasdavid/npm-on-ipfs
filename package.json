{
  "name": "ipfs-npm",
  "version": "0.6.3",
  "description": "Set up a NPM registry mirror, using your favourite storage, IPFS! :D",
  "main": "src/index.js",
  "bin": {
    "ipfs-npm": "src/cli/bin.js",
    "ipnpm": "src/cli/bin.js"
  },
  "scripts": {
    "lint": "aegir-lint",
    "test": "aegir-test --env node",
    "release": "aegir-release --env no-build",
    "release-minor": "aegir-release --type minor --env no-build",
    "release-major": "aegir-release --type major --env no-build",
    "coverage": "aegir-coverage",
    "coverage-publish": "aegir-coverage publish"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diasdavid/ipfs-npm.git"
  },
  "keywords": [
    "IPFS",
    "npm",
    "npm-on-ipfs",
    "libp2p"
  ],
  "author": "David Dias <daviddias@ipfs.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diasdavid/ipfs-npm/issues"
  },
  "homepage": "https://github.com/diasdavid/ipfs-npm#readme",
  "dependencies": {
    "async": "^2.1.2",
    "debug": "^2.2.0",
    "dnscache": "0.0.4",
    "express": "^4.14.0",
    "follow-registry": "^3.1.0",
    "ipfs-api": "^9.0.0",
    "ipfs-blob-store": "^1.1.1",
    "lru-cache": "^4.0.1",
    "multiaddr": "^2.0.3",
    "once": "^1.4.0",
    "patch-package-json": "0.0.4",
    "read-pkg-up": "^2.0.0",
    "request": "^2.76.0",
    "sha": "^2.0.1",
    "st": "^1.2.0",
    "timethat": "0.0.3",
    "update-notifier": "^1.0.2",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "abstract-blob-store": "^3.2.0",
    "aegir": "^8.1.2",
    "assert": "^1.4.1",
    "chai": "^3.5.0",
    "ipfsd-ctl": "^0.16.0",
    "mockery": "^2.0.0",
    "ncp": "^2.0.0",
    "pre-commit": "^1.1.3",
    "prettysize": "0.0.3",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  }
}
